        "@testing-library/jest-dom": "^6.7.0",
        "@testing-library/svelte": "^5.2.8",
        "autoprefixer": "^10.4.21",
        "jsdom": "^26.1.0",
        "postcss": "^8.5.6",
        "svelte": "^5.0.0",
        "svelte-check": "^4.0.0",
        "tailwindcss": "^3.4.17",
        "typescript": "^5.0.0",
        "vite": "^7.0.4",
        "vitest": "^3.2.4"
